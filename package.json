{
  "name": "@chuck1/silver-ui",
  "type": "module",
  "version": "1.0.0",
  "description": "vue3 component library",
  "author": {
    "name": "Chuck"
  },
  "license": "MIT",
  "homepage": "http://139.9.177.45:3333",
  "repository": {
    "type": "git",
    "url": "git@github.com:chuckbiu/Silver-UI.git"
  },
  "keywords": [
    "component",
    "UI",
    "vue3"
  ],
  "sideEffects": ["dist/si-element.css"],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/si-element.js",
      "require": "./dist/umd/si-element.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },

  "main": "./dist/umd/si-element.umd.cjs",
  "module": "./dist/es/si-element.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/si-element.css dist/si-element.css",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.4.0",
    "axios": "^1.8.2",
    "eslint-plugin-format": "^1.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "rollup": "^4.40.0",
    "unplugin-vue-macros": "^2.14.5",
    "vitest": "^3.0.8",
    "vue": "^3.5.13"

  },

  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/lodash": "^4.17.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^9.30.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "postcss-each": "^1.1.0",
    "typescript": "~5.6.3",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vitepress": "^1.6.3",
    "vue-tsc": "^2.1.10"
  }
}
